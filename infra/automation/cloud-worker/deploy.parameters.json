{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "baseName": {
      "value": "matoolkit"
    },
    "targetTenantId": {
      "value": "<TARGET-TENANT-ID>"
    },
    "appId": {
      "value": "<APP-REGISTRATION-CLIENT-ID>"
    },
    "targetOrganization": {
      "value": "<TARGET-TENANT-DOMAIN>"
    },
    "workerId": {
      "value": "worker-01"
    },
    "maxParallelism": {
      "value": 4
    },
    "containerImage": {
      "value": "matoolkitacr.azurecr.io/cloud-worker:latest"
    },
    "containerCpu": {
      "value": "1.0"
    },
    "containerMemory": {
      "value": "2.0Gi"
    },
    "idleTimeoutSeconds": {
      "value": 300
    },
    "cloudWorkerSubnetId": {
      "value": "/subscriptions/<SUBSCRIPTION_ID>/resourceGroups/<RESOURCE_GROUP>/providers/Microsoft.Network/virtualNetworks/matoolkit-vnet/subnets/snet-cloud-worker"
    },
    "serviceBusNamespaceName": {
      "value": "matoolkit-sbus"
    },
    "keyVaultName": {
      "value": "matoolkit-kv"
    },
    "containerRegistryName": {
      "value": "matoolkitacr"
    },
    "logAnalyticsWorkspaceName": {
      "value": "matoolkit-logs"
    },
    "tags": {
      "value": {
        "component": "cloud-worker",
        "project": "ma-toolkit",
        "environment": "dev"
      }
    }
  }
}
